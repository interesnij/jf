                      <div class="text-gray ml-auto mt-auto">Optional</div>
                    </div>
                  </div>
                  <div class="col-6">
                    <div class="sc-fbNZge kcjhkj">
                      <div class="flex-1 d-flex">
                        <div class="sc-GTVdH dXaChH">
                            <input id="u83EpQCPy2G8p_XgO_X1G" pattern="[0-9.]+" placeholder="Enter your hourly rate" value="1.00">
