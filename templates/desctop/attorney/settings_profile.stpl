                              <div class="select-control__menu-item" data-pk="2"><span>GBP</span></div>
                              <div class="select-control__menu-item" data-pk="3"><span>ERU</span></div>
                          </div>
                        </div>
                        <div class="select-control__footer"></div>
                      </div>
                    </div>
