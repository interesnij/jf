                    <div class="d-flex justify-content-between">
                      <div class="multi-select-control__label"></div>
                    </div>
                    <div class="multi-select-control__container">
                      <div class="multi-select-control active">
                        <div class="flex-1 d-flex flex-wrap">
                            <!-- active languages -->
                          <div class="multi-select-control__item">
                            <span class="my-auto">English</span>
                            <img src="/static/images/close.svg" alt="close" class="multi-select-control__item-close">
                          </div>  
                          <!-- active languages -->
                        </div>
                      </div>
                      <div class="multi-select-control__menu">
                        <!-- languages -->
                        <div class="multi-select-control__menu-item active"><span>English</span></div>
                        <div class="multi-select-control__menu-item"><span>Greek</span></div>
                        <!-- languages -->
                      </div>
                    </div>
                    <div class="multi-select-control__footer"></div>
                  </div>
                  <div class="heading mt-3 col-12">Do you want to submit proposals for potential client engagements?</div>
                  <div class="col-12">
                    <div>
                      <div class="d-flex flex-wrap">
                        <label class="radio-control">Yes<input type="radio" name="is_submittable_potential" checked="">
                            <span></span>
                        </label>
                        <label class="radio-control">No<input type="radio" name="is_submittable_potential">
                            <span></span>
                        </label>
                      </div>
                    </div>
                  </div>
                  <div class="heading mt-3 w-100">
                    <div class="col-6 d-flex"><span class="mb-1">Tax Rate</span>
                      <div class="text-gray ml-auto mt-auto">Optional</div>
                    </div>
