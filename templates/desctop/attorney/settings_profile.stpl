                        <label class="checkbox-component">In person<input type="checkbox" checked=""><span></span></label>
                      </div>
                      <div class="mr-2">
                        <label class="checkbox-component">JusLaw VC<input type="checkbox" checked=""><span></span></label>
                      </div>
                    </div>
                  </div>
