                      </div>
                      <input id="gLodlDNvk1OH6d4htdVzQ" placeholder="Input middle name here" type="text" class="active" value="A">
                      <div class="input-control__footer"></div>
                    </div>
                    <div class="input-control mt-2">
                      <div class="d-flex justify-content-between align-items-center">
                        <label for="4AlZ5v8UayGq13fJiYBsi" class="input-control__label">Last Name</label>
                      </div>
                      <input id="4AlZ5v8UayGq13fJiYBsi" placeholder="Input last name here" type="text" class="active" value="G">
                      <div class="input-control__footer"></div>
                    </div>
                    <div class="input-control mt-2">
                      <div class="d-flex justify-content-between align-items-center">
                        <label for="Ky6qvgTmCHJ9g8pSo2FAg" class="input-control__label">Email</label>
                      </div>
                      <input id="Ky6qvgTmCHJ9g8pSo2FAg" placeholder="Input email here" type="text" class="active" value="sergejuslaw@gmail.com">
                      <div class="input-control__footer"></div>
                    </div>
                    <div class="input-control mt-2">
                        <div class="d-flex justify-content-between align-items-center">
                            <label for="id_phone" class="input-control__label">Phone</label>
                        </div>
                        <input id="id_phone" name="phone" placeholder="+1 (123) 456 - 7890" type="tel">
                        <div class="input-control__footer"></div>
                    </div>
                  </div>
                  <div class="col-md-6 mt-3">
                    <div class="textarea-form-control d-flex flex-column h-100">
                      <div class="d-flex justify-content-between">
                        <div class="textarea-form-control__label">Biography</div>
                        <div class="textarea-form-control__required">Optional</div>
                      </div>
                      <textarea placeholder="Describe your legal practice" class="active flex-1">description</textarea>
                      <div class="textarea-form-control__footer"></div>
                    </div>
                  </div>
                </div>
