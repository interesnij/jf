<% include!("../anon_header.stpl"); %>

<div class="signup-page doc_title" data-title="<%= title %>" data-description="<%= description %>" data-uri="<%= link %>" data-image="<%= image %>">
    <div class="create-account-layout">
        <div class="create-account-layout__side">
        <a href="/"><img src="data:image/png;base64," class="create-account-layout__logo" alt="logo"></a>
            <div class="create-account-layout__label mt-auto"></div>
            <h1>Law Firm</h1>
            <div class="create-account-layout__side-content my-4"></div>
            <div class="create-account-layout__features">Features</div>
            <div class="create-account-layout__side-content mb-auto">
                <ul>
                    <li>No hidden fees – every JusLaw feature included</li>
                    <li>Safe &amp; secure – only platform with patented blockchain technology</li>
                    <li>End-to-end solution – only platform to provide an end-to-end solution for you &amp; your clients</li>
                    <li>Exceptional service for all – no tier-based service</li>
                </ul>
            </div>
            <div class="link-button"><img src="/static/images/arrow-left-green.svg" alt="icon">Go Back</div>
        </div>
        <div class="create-account-layout__content">
            <div class="my-auto d-flex flex-column">
                <div class="create-account-layout__form mx-auto">
                    <form action="#">
                        <div class="step mb-2">Step 1 of 2</div>
                        <div class="title mb-3">Create your account</div>
                        <div class="input-control mt-1">
                            <div class="d-flex justify-content-between align-items-center"><label for="id_email" class="input-control__label">Email</label></div><input id="id_email" name="email" placeholder="Enter your email address" type="email">
                            <div class="input-control__footer"></div>
                        </div>
                        <div class="input-control mt-3">
                            <div class="d-flex justify-content-between align-items-center"><label for="id_password" class="input-control__label">Password</label></div><input id="id_password" placeholder="Create password" type="password" name="password">
                            <div class="input-control__footer">
                                <div class="input-control__help ml-auto">Minimum of 8 characters, 1 number, and 1 symbol</div>
                            </div>
                        </div>
                        <div class="input-control mt-3">
                            <div class="d-flex justify-content-between align-items-center"><label for="id_password2" class="input-control__label">Confirm Password</label></div><input id="id_password2" placeholder="Confirm your password" type="password" name="password">
                            <div class="input-control__footer"></div>
                        </div>
                        <div class="d-flex mt-4 mb-2"><button class="btn btn--green ripple-effect large" type="button" style="width: 100%;"><span>Next</span></button></div>
                    </form>
                </div>
            </div>
            <div class="login-page__footer mb-3"><span class="mr-1">Already have an account?</span><a href="/auth/login">Log in here</a></div>
        </div>
    </div>
</div>

<% include!("../anon_footer.stpl"); %>